{
  "expo": {
    "name": "signal-clone",
    "slug": "signal-clone",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./signal-logo.png",
    "sdkVersion": "41.0.0",
    "splash": {
      "image": "./signal-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.HarshAppDeveloper.Signal",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./signal-logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./signal-logo.png"
    }
  }
}
